{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Display and manage tabs",
  "permissions": ["tabs", "storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "img/icon48.png",
    "64": "img/icon64.png",
    "128": "img/icon128.png"
  }
}